{
  "compilerOptions": {
    "esModuleInterop": true,
    "lib": ["esnext"],
    "module": "commonjs",
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": true,
    "target": "esnext",
    "strictFunctionTypes": true
  }
}
